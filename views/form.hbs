  <!-- multistep form -->
    <form id="msform">
        <!-- progressbar -->
        <ul id="progressbar">
          <li class="active"></li>
          <li></li>
          <li></li>
        </ul>
        <!-- fieldsets -->
        <fieldset>
          <h2 class="fs-title">Step 1</h2>
          <h3 class="fs-subtitle">Where is your provider?</h3>
          <!--<p class="help-block">List your strengths here.</p>-->
          <label for="inputCity">Enter Zip Code or City, State</label>
          <input type="text" class="form-control" id="inputCity" placeholder="Enter Zip Code or City, State">
          <input type="button" name="next" class="next action-button" value="Next" />
        </fieldset>
    
        <fieldset>
          <h2 class="fs-title">Step 2</h2>
          <h3 class="fs-subtitle">What type of care do you need?</h3>
          <input type="button" name="mri" class="next action-button" id="mri" value="MRI" />
          <input type="button" name="ct" class="next action-button" id="ct" value="CT (Cat) Scan" />
          <p class="or">OR</p>
          <label for="inputCpt">Already know your Procedure Code? Enter Below</label>
          <input type="text" class="form-control" id="inputCpt" placeholder="Enter a Procedure Code">
          <input type="button" name="previous" class="previous action-button" value="Previous" />
          <input type="button" name="next" class="next action-button" value="Next" />
        </fieldset>
    
        <fieldset>
          <h2 class="fs-title">Step 3</h2>
          <h3 class="fs-subtitle">Select <b>one</b> code</h3>
            {{#each this}} 
                <tr class="scanRow"> 
                    <input class="form-check-input" type="radio" name="exampleRadios" id="exampleRadios1" value="option1" checked>
                    <td id ='id'>{{id}} </td>
                    <td id="cpt">{{cpt}}</td>
                    <td id="description"> {{description}}</td>
                </tr>
            {{/each}}
          <input type="button" name="previous" class="previous action-button" value="Previous" />
          <input type="button" name="next" id="requestCmsData"class="next action-button" value="Get your cost" />
        </fieldset>
      </form>
    <link rel="stylesheet" href="/css/styles.css">
      <!-- files that were in the head -->
    <script src="https://code.jquery.com/jquery-3.3.1.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.js"></script>
      <!-- jQuery -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.9.1/jquery.min.js" type="text/javascript"></script>
    <!-- jQuery easing plugin -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js" type="text/javascript"></script>
    <!-- custom script -->
    <script src="/js/form.js"></script>
      